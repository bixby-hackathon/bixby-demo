layout-macro-def(pokemon-evolutions-macro) {
  params {
    param (pokemonEvolutions) {
      type (PokemonEvolutions)
      min (Required)
      max (One)
    }
  }

  content {
    compound-card {
      content {
        title-area {
          slot1 {
            text {
              value {
                template ("Hello I'm #{value (pokemonEvolutions.name)}")
              }
            }
          }
        }
      }
    }
  }
}
